#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules/@graphql-codegen/cli/cjs/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules/@graphql-codegen/cli/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules/@graphql-codegen/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules/@graphql-codegen/cli/cjs/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules/@graphql-codegen/cli/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules/@graphql-codegen/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/@graphql-codegen+cli@5.0.4_@babel+core@7.27.4_@types+node@20.14.15_graphql@16.11.0/node_modules:/Users/espoir/dev-work/bianyi/javas/rafiki/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/cjs/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/cjs/bin.js" "$@"
fi
